<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">

	<display-name>sleepy-app</display-name>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>FirstServlet</servlet-name>
		<servlet-class>com.skillsimprover.startload.servlets.FirstServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>FirstServlet</servlet-name>
		<url-pattern>/first.html</url-pattern>
	</servlet-mapping>


	<servlet>
		<servlet-name>SecondServlet</servlet-name>
		<servlet-class>com.skillsimprover.startload.servlets.SecondServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>SecondServlet</servlet-name>
		<url-pattern>/second.html</url-pattern>
	</servlet-mapping>



	<servlet>
		<servlet-name>ThirdServlet</servlet-name>
		<servlet-class>com.skillsimprover.startload.servlets.ThirdServlet</servlet-class>
		<load-on-startup>3</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>ThirdServlet</servlet-name>
		<url-pattern>/third.html</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>FourthServlet</servlet-name>
		<servlet-class>com.skillsimprover.startload.servlets.FourthServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>FourthServlet</servlet-name>
		<url-pattern>/fourth.html</url-pattern>
	</servlet-mapping>
</web-app>
